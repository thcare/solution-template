{
  "private": true,
  "name": "solution-template",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Thrive Health",
  "type": "module",
  "main": "dist/src/solution.js",
  "files": [
    ".funcignore",
    "Fn*/**",
    "assets",
    "components/package.json",
    "dist",
    "host.json",
    "local.settings.json",
    "prisma"
  ],
  "scripts": {
    "build": "wireit",
    "bundle": "wireit",
    "dev": "wireit",
    "lint": "wireit",
    "open-browser": "[ \"${GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN}\" ] && ( sensible-browser \"https://${CODESPACE_NAME}-3000.${GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN}/\" ) || ( sensible-browser http://localhost:3000 )",
    "start": "wireit",
    "workbench": "yarn run dev --watch"
  },
  "types": "dist/src/solution.d.ts",
  "dependencies": {
    "@azure/functions": "^3.5.1",
    "@thcare/thfx": "^27.2.0",
    "@thcare/thschemix": "^27.2.0",
    "@types/invariant": "^2.2.35",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "invariant": "^2.2.4",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "urql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@prisma/client": "^4.16.2",
    "@thcare/eslint-config-thcare": "^26.12.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/graphql-fields": "^1.3.5",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.17",
    "eslint": "^8.47.0",
    "graphql": "^16.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.2",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "type-graphql": "2.0.0-beta.3",
    "typegraphql-prisma": "^0.26.0",
    "typescript": "^5.2.2",
    "wireit": "^0.10.0"
  },
  "devEngines": {
    "node": "18.x"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build/prisma",
        "build/graphql",
        "build/tsc",
        "build/compile"
      ]
    },
    "build/prisma": {
      "command": "thfx prisma",
      "files": [
        "schema/**/*.ts"
      ],
      "output": [
        "prisma/schema.prisma",
        "schema.gql"
      ]
    },
    "build/graphql": {
      "command": "graphql-codegen",
      "files": [
        "codegen.yml",
        "schema.gql"
      ],
      "dependencies": [
        "build/prisma"
      ]
    },
    "build/tsc": {
      "command": "tsc --build",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.{ts,tsx}",
        "tsconfig.json"
      ],
      "output": [
        "dist/**",
        ".tsbuildinfo"
      ],
      "dependencies": [
        "build/prisma",
        "build/graphql"
      ]
    },
    "build/compile": {
      "command": "thfx solution:compile --solution-file ./dist/src/solution.js",
      "dependencies": [
        "build/tsc"
      ]
    },
    "lint": {
      "command": "eslint src --ext .ts --ext .tsx --ignore-pattern 'src/generated/**'",
      "files": [
        "src/**/*.{ts,tsx}",
        "tsconfig.json"
      ],
      "dependencies": [
        "build/prisma",
        "build/graphql"
      ]
    },
    "start": {
      "command": "func start",
      "service": true,
      "files": [
        "local.settings.json"
      ],
      "dependencies": [
        {
          "script": "build",
          "cascade": false
        }
      ]
    },
    "dev": {
      "command": "thfx solution:dev",
      "service": true,
      "dependencies": [
        "build",
        "start"
      ]
    },
    "bundle": {
      "command": "thfx solution:bundle --targetDir .",
      "dependencies": [
        "build"
      ]
    }
  },
  "packageManager": "yarn@4.0.0-rc.51"
}
