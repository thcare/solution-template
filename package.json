  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/thcare/solution-template/issues"
  },
  "homepage": "https://github.com/thcare/solution-template#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@thcare/thcarefx": "^6.8.2",
    "@types/node": "^20.2.3",
    "husky": "^8.0.3",
    "pretty-quick": "^3.1.3",
    "typegraphql-prisma": "^0.25.1",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
